<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="90dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clRouteInfo1"
        android:background="@drawable/row_contents_routeinfo_bg"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/clRouteInfo2">


        <TextView
            android:id="@+id/tvOptionName1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text="contents"
            android:textColor="@color/black01"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/tvDist1"/>

        <TextView
            android:id="@+id/tvDist1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvOptionName1"
            app:layout_constraintBottom_toTopOf="@id/tvTime1"/>

        <TextView
            android:id="@+id/tvTime1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvDist1"
            app:layout_constraintBottom_toTopOf="@id/tvFee1"/>

        <TextView
            android:id="@+id/tvFee1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvTime1"
            app:layout_constraintBottom_toBottomOf="parent"/>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clRouteInfo2"
        android:background="@drawable/row_contents_routeinfo_bg"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toRightOf="@id/clRouteInfo1"
        app:layout_constraintRight_toLeftOf="@id/clRouteInfo3">

        <TextView
            android:id="@+id/tvOptionName2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text="contents"
            android:textColor="@color/black01"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/tvDist2"/>

        <TextView
            android:id="@+id/tvDist2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvOptionName2"
            app:layout_constraintBottom_toTopOf="@id/tvTime2"/>

        <TextView
            android:id="@+id/tvTime2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvDist2"
            app:layout_constraintBottom_toTopOf="@id/tvFee2"/>

        <TextView
            android:id="@+id/tvFee2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvTime2"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clRouteInfo3"
        android:background="@drawable/row_contents_routeinfo_bg"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toRightOf="@id/clRouteInfo2"
        app:layout_constraintRight_toRightOf="parent">

        <TextView
            android:id="@+id/tvOptionName3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text="contents"
            android:textColor="@color/black01"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/tvDist3"/>

        <TextView
            android:id="@+id/tvDist3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvOptionName3"
            app:layout_constraintBottom_toTopOf="@id/tvTime3"/>

        <TextView
            android:id="@+id/tvTime3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvDist3"
            app:layout_constraintBottom_toTopOf="@id/tvFee3"/>

        <TextView
            android:id="@+id/tvFee3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="contents"
            android:textColor="#ff797979"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/tvTime3"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>